package com.pms.company.helper;

import com.pms.company.bean.Company;
import com.pms.company.modal.CompanyModal;

public class CompanyHelper {
	
	
	public Company createCompanyEntity(CompanyModal companyModal){
		Company company = new Company();
		companyModal.setCompanyName(companyName);
		companyModal.setDateOfIncorporation(dateOfIncorporation);
		companyModal.setDisplayName(displayName);
		companyModal.setEmail(email);
		companyModal.setId(id);
		companyModal.setIsDealer(isDealer);
		companyModal.setIsRetailer(isRetailer);
		companyModal.setLandLineNo(landLineNo);
		companyModal.setLogo(logo);
		companyModal.setSignatureInNameOf(signatureInNameOf);
		companyModal.setMobileNo(mobileNo);
		return null;
	}

}
