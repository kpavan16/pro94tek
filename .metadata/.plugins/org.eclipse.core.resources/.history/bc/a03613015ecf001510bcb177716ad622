public class CategoryHelper {
	
	
	public CategoryModal createCategoryModal(Category category){
		CategoryModal categoryModal = new CategoryModal();
		categoryModal.setCategoryName(categoryName);
		categoryModal.setCreatedBy(createdBy);
		categoryModal.setCreatedDate(createdDate);
		categoryModal.setDescription(description);
		categoryModal.setId(id);
		
		
		return null;
	}

}
